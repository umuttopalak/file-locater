{
  "name": "file-locater",
  "displayName": "File Locater",
  "description": "This is an file locater which is file is open ",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.8.0"
  },
  "publisher": "UmutTopalak",
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/umuttopalak/file-locater"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "file-locater.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "file-locater.locateFile",
        "title": "Locate File",
        "icon": {
          "light": "resources/light-icon.svg",
          "dark": "resources/dark-icon.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "file-locater.locateFile",
          "when": "view == explorer",
          "group": "navigation"
        }
      ],
      "editor/title": [
        {
          "command": "file-locater.locateFile",
          "group": "navigation"
        }
      ],
      "explorer/title": [
        {
          "command": "file-locater.locateFile",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "esbuild": "^0.25.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
